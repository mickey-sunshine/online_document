      <file>${ROOT_PATH}/etc/device/ALKDC/synth/sdc/common/dont_use_cell.tcl</file>
      <file>${ROOT_PATH}/etc/device/ALKDC/synth/sdc/io/area.tcl</file>
      <file>${ROOT_PATH}/etc/device/ALKDC/synth/sdc/io/timing.tcl</file>
      <file>${ROOT_PATH}/etc/device/ALKDC/synth/sdc/io/power.tcl</file>
    </sdc>
  </strategy>
  <strategy name="io_dchain">
    <designs>
      <pattern>grid_io_bottomL_bottom</pattern>
    </designs>
    <target_lib>
      <lib>uhdc40_hvt_v0.2</lib>
      <lib>uhdc40_rvt_v0.2</lib>
    </target_lib>
    <compile type="ultra" optimization="area" effort="high"/>
    <sdc>
      <file>${ROOT_PATH}/etc/device/ALKDC/synth/sdc/common/constants.tcl</file>
      <file>${ROOT_PATH}/etc/device/ALKDC/synth/sdc/common/dont_use_cell.tcl</file>
      <file>${ROOT_PATH}/etc/device/ALKDC/synth/sdc/io/area_dchain.tcl</file>
      <file>${ROOT_PATH}/etc/device/ALKDC/synth/sdc/io/timing.tcl</file>
      <file>${ROOT_PATH}/etc/device/ALKDC/synth/sdc/io/power.tcl</file>
